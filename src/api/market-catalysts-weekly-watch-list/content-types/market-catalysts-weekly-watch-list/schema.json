{
  "kind": "collectionType",
  "collectionName": "market_catalysts_weekly_watch_lists",
  "info": {
    "singularName": "market-catalysts-weekly-watch-list",
    "pluralName": "market-catalysts-weekly-watch-lists",
    "displayName": "Market Catalysts Weekly Watch List",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Publication": {
      "type": "date"
    },
    "introduction": {
      "type": "richtext"
    },
    "marketoverview": {
      "type": "richtext"
    },
    "sp500Chart": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "catalystItems": {
      "displayName": "Catalyst Item",
      "type": "component",
      "repeatable": true,
      "component": "market-catalysts.catalyst-item"
    },
    "featuredCatalysts": {
      "type": "richtext"
    },
    "summary": {
      "type": "richtext"
    },
    "CkEditor": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "Editor2": {
      "type": "customField",
      "options": {
        "output": "Markdown",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    }
  }
}
